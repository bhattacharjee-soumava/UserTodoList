<%- include("partials/header") -%>



<div class="top-margin">
  <div class="container">
    <div class="row">
      <div class="col-6">
        <div class="auth-control">
          <div class="card social-block">
            <div class="card-body">
              <a class="btn btn-block btn-social btn-google" href="/auth/google" role="button">
                <i class="fab fa-google"></i>
                Sign In with Google
              </a>
            </div>
          </div>
          <div class="card">
            <div class="card-body">
              <a class="btn btn-block btn-social btn-facebook" href="/auth/facebook" role="button">
                <i class="fab fa-facebook"></i>
                Sign In with Facebook
              </a>
            </div>
          </div>
        </div>
      </div>
      <div class="col-6">
        <form class="form-signin" action="/signin" method="post">
          <!-- <img class="mb-4" src="https://getbootstrap.com/docs/4.0/assets/brand/bootstrap-solid.svg" alt="" width="72" height="72"> -->
          <!-- <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1> -->
          <label for="inputEmail" class="sr-only">Email address</label>
          <input type="email" id="inputEmail" class="form-control" name="username" placeholder="Email address" required autofocus autocomplete="off">
          <label for="inputPassword" class="sr-only">Password</label>
          <input type="password" id="inputPassword" class="form-control" name="password" placeholder="Password" required autocomplete="off">
          <div class="checkbox mb-3 text-color">
            <label>
              <input type="checkbox" value="remember-me"> Remember me
            </label>
          </div>
          <button class="btn btn-lg btn-primary btn-block" type="submit" name="button">Sign in</button>


        </form>
        <form class="form-signup" action="/registerpage" method="post">
          <button class="btn btn-lg btn-primary btn-block" type="submit" name="Sign Up">Sign Up</button>
        </form>
      </div>
    </div>
  </div>
</div>





<%- include("partials/footer") -%>
